{
  "name": "dlx-mcp-server",
  "version": "0.0.1",
  "description": "dlx-mcp-server MCP server",
  "type": "module",
  "bin": {
    "dlx-mcp-server": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "start": "node dist/index.js",
    "dev": "tsc --noEmit && node --loader ts-node/esm src/index.ts",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "get-token": "tsc --noEmit && node --loader ts-node/esm src/scripts/get-token.ts",
    "check-realm": "tsc --noEmit && node --loader ts-node/esm src/scripts/check-realm.ts",
    "get-client-token": "tsc --noEmit && node --loader ts-node/esm src/scripts/get-client-token.ts",
    "get-auth-url": "tsc --noEmit && node --loader ts-node/esm src/scripts/get-auth-url.ts",
    "get-token-with-code": "tsc --noEmit && node --loader ts-node/esm src/scripts/get-token-with-code.ts"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.9.0",
    "@types/axios": "^0.9.36",
    "axios": "^1.8.4",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "open": "^9.1.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.24",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.19.0"
  }
}